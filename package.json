{
  "name": "mystjs",
  "version": "0.0.12",
  "description": "Markdown parser for MyST markdown in JavaScript",
  "author": "Rowan Cockett <rowanc1@gmail.com>",
  "homepage": "https://github.com/executablebooks/mystjs",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/index.umd.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/esm/index.js"
    }
  },
  "keywords": [
    "markdown-it-plugin",
    "markdown-it",
    "markdown",
    "myst",
    "jupyter",
    "jupyter-book"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/executablebooks/mystjs.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "npm run clean && tsc -p .",
    "prepublishOnly": "npm run compile; npm run build;",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "test:watch": "node --experimental-vm-modules ./node_modules/.bin/jest --watchAll",
    "test:clear": "node --experimental-vm-modules ./node_modules/.bin/jest --clearCache",
    "lint": "eslint \"src/**/*.ts\" -c .eslintrc.json --max-warnings 1",
    "lint:format": "prettier --write src/*.ts src/**/*.ts",
    "lint:format:fix": "eslint \"src/**/*.ts\" -c .eslintrc.json --fix",
    "watch": "tsc -w -p .",
    "test:cov": "jest --coverage",
    "build:bundles": "rollup -c",
    "build:esm": "tsc --module es2015   --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "declarations": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "build": "npm-run-all -l clean -p build:cjs build:esm build:bundles declarations",
    "build:watch": "rollup -c -w; npm run sass -w"
  },
  "bugs": {
    "url": "https://github.com/executablebooks/mystjs/issues"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "hast-util-to-mdast": "^8.3.1",
    "he": "^1.2.0",
    "markdown-it": "^12.3.2",
    "markdown-it-amsmath": "^0.3.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-docutils": "0.1.6",
    "markdown-it-dollarmath": "^0.4.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-myst-extras": "0.2.0",
    "markdown-it-task-lists": "^2.1.1",
    "mdast-util-find-and-replace": "^2.1.0",
    "mdast-util-to-hast": "^12.1.1",
    "myst-spec": "^0.0.4",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-remark": "^9.1.2",
    "rehype-stringify": "^9.0.3",
    "unified": "^10.1.1",
    "unist-builder": "^3.0.0",
    "unist-util-find-after": "^4.0.0",
    "unist-util-map": "^3.0.1",
    "unist-util-remove": "^3.1.0",
    "unist-util-select": "^4.0.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@sinonjs/fake-timers": "^9.1.0",
    "@types/hast": "^2.3.4",
    "@types/he": "^1.1.2",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it": "^12.2.3",
    "@types/mdast": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-skip-tests": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.2",
    "express": "^4.17.3",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3"
  }
}

{
  "name": "myst-to-docx",
  "version": "0.0.24",
  "description": "Export from a MyST Markdown document to Microsoft Word (*.docx)",
  "author": "Rowan Cockett <rowan@curvenote.com>",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "myst-plugin",
    "docx",
    "word",
    "export"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/executablebooks/mystjs.git"
  },
  "bugs": {
    "url": "https://github.com/executablebooks/mystjs/issues"
  },
  "homepage": "https://github.com/executablebooks/mystjs/tree/main/packages/myst",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\" -c .eslintrc.cjs",
    "lint:format": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build:esm": "tsc --project ./tsconfig.json --module es2015 --outDir dist --declaration",
    "build": "npm-run-all -l clean -p build:esm"
  },
  "dependencies": {
    "buffer-image-size": "^0.6.4",
    "docx": "^7.3.0",
    "myst-common": "^0.0.17",
    "myst-frontmatter": "^0.0.14",
    "myst-spec": "^0.0.4",
    "myst-spec-ext": "^0.0.12"
  },
  "devDependencies": {}
}
